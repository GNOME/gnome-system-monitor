<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="GtkListItem">
    <property name="child">
      <object class="GtkLabel">
        <property name="xalign">1</property>
        <binding name="label">
          <closure type="gchararray" function="format_size">
            <lookup name="available" type="GsmDisk">
